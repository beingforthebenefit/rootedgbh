---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Hero from '../components/Hero.astro';
import Services from '../components/Services.astro';
import About from '../components/About.astro';
import ABASection from '../components/ABASection.astro';
import TreatmentPlans from '../components/TreatmentPlans.astro';
import Locations from '../components/Locations.astro';
import Team from '../components/Team.astro';
import FAQ from '../components/FAQ.astro';
import Insurance from '../components/Insurance.astro';
import Contact from '../components/Contact.astro';

const baseUrl = import.meta.env.BASE_URL === '/' ? '' : import.meta.env.BASE_URL;
---

<Layout title="Rooted Growth Behavioral Health">
  <Header />
  <main>
    <Hero />
    <About />
    <Services />
    <ABASection />
    <TreatmentPlans />
    <Locations />
    <Team />
    <FAQ />
    <Insurance />
    <Contact />
  </main>
</Layout>